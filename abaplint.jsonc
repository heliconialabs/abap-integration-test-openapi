          "replace": "use POPUP_TO_CONFIRM"
        },
        {
          "name": "POPUP_TO_DECIDE",
          "replace": "use POPUP_TO_CONFIRM"
        },
        {
          "name": "POPUP_TO_GET_VALUE",
          "replace": "use POPUP_GET_VALUES"
        },
        {
